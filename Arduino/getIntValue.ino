int getIntVal(String strVal){
  int myVal;
  if(strVal == "0"){
    myVal = 0;
  } else if(strVal == "1") {
    myVal = 1;
  } else if(strVal == "2") {
    myVal = 2;
  } else if(strVal == "3") {
    myVal = 3;
  } else if(strVal == "4") {
    myVal = 4;
  } else if(strVal == "5") {
    myVal = 5;
  } else if(strVal == "6") {
    myVal = 6;
  } else if(strVal == "7") {
    myVal = 7;
  } else if(strVal == "8") {
    myVal = 8;
  } else if(strVal == "9") {
    myVal = 9;
  }
  return myVal;
}
